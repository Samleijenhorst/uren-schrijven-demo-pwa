{
	"_version": "1.12.0",
	"name": "PWA Demo",
	"short_name": "ToDo",
	"start_url": "index.html",
	"display": "standalone",
	"background_color": "#FFFFFF",
	"theme_color": "#FFFFFF",
	"gcm_sender_id": "512591402163",
	"icons": [
		{
			"src": "image/Icon-144.png",
			"sizes": "144x144",
			"type": "image/png"
		},
		{
			"src": "image/Icon-192.png",
			"sizes": "192x192",
			"type": "image/png"
		},
		{
			"src": "image/Icon-36.png",
			"sizes": "36x36",
			"type": "image/png"
		},
		{
			"src": "image/Icon-48.png",
			"sizes": "48x48",
			"type": "image/png"
		},
		{
			"src": "image/Icon-512.png",
			"sizes": "512x512",
			"type": "image/png"
		},
		{
			"src": "image/Icon-96.png",
			"sizes": "96x96",
			"type": "image/png"
		},
		{
			"src": "image/Icon-144.png",
			"sizes": "144x144",
			"type": "image/png"
		}
	],
	"sap.app": {
		"id": "com.alliander.todo.demo.uren-schrijven-demo-pwa",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.alliander.todo.demo.uren-schrijven-demo-pwa.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.alliander.todo.demo.uren-schrijven-demo-pwa.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.alliander.todo.demo.uren-schrijven-demo-pwa.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "Master",
					"pattern": "",
					"target": [
						"Master"
					]
				},
				{
					"name": "Detail",
					"pattern": "/{Item}",
					"target": [
						"Detail"
					]
				}
			],
			"targets": {
				"Master": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "Master",
					"viewName": "Master"
				},
				"Detail": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "Detail",
					"viewName": "Detail"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}